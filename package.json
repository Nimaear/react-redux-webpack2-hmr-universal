{
  "name": "coursio-store",
  "version": "0.0.1",
  "description": "Univeral JS application utilizing Hot Module Reloading, Server Side Rendering, Code Splitting with React, Redux, React Router 4 and a heroku like deploy setup with Dokku.",
  "main": "index.js",
  "scripts": {
    "development": "NODE_PATH=./src NODE_ENV=development node ./src/server/server.babel.js",
    "production": "NODE_PATH=./src NODE_ENV=production node ./src/server/server.babel.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "webpack --config ./webpack/server.babel.js",
    "build:client": "webpack --config ./webpack/client.babel.js"
  },
  "keywords": [
    "react",
    "redux",
    "hot-module-reloading",
    "server-side-rendering",
    "hmr",
    "ssr",
    "code-splitting",
    "webpack"
  ],
  "author": "Özgür Ayten",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.28.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.18.0",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-plugin-oxygen-css": "^1.0.3",
    "babel-plugin-oxygen-i18n": "^1.0.4",
    "cio": "ssh://github.com/Coursio/cio",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "normalizr": "^3.2.2",
    "oxygen-debounce": "^1.0.1",
    "oxygen-i18n": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-transition-group": "^1.1.3",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0"
  }
}
